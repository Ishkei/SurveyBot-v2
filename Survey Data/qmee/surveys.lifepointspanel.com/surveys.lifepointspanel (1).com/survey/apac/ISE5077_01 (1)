<script>
jQuery(document).ready(
  function() {
    /* survey.xml line 496 */
    jQuery(document).ready(function($) {
  // 禁用粘贴事件 ---------------------------------
  $ (document).on('paste', 'input, textarea', function(e) {
    e.preventDefault();
    alert("Pasting is prohibited, please enter manually!");
  });
  // 防御性监控：防止绕过粘贴事件的操作 ---------------
  let lastVal = "";
  let turncon=0;
  $ (document) .on('input propertychange', 'input, textarea', function() {
      const newVal = $ (this).val();
      console.log(newVal); 

    // 检测内容突变（如拖拽文本、浏览器自动填充）
    if (newVal.length - lastVal.length > 5) { 
      $ (this).val(lastVal);
      alert("Illegal input detected!");
      return;
    }
    lastVal = newVal;
  });
});
  }
);
</script>


