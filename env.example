# Google Gemini API Key (Required for AI features)
GOOGLE_API_KEY=your_gemini_api_key_here

# Bot Configuration
BROWSER_TYPE=v2ray
SURVEY_PLATFORM=qmee
HEADLESS=false

# Proxy Settings
USE_PROXY=true
PROXY_ROTATION=true

# AI Settings
USE_VISION=true
USE_FALLBACK=true

# Timing Settings
RANDOM_DELAYS=true
MIN_DELAY=1
MAX_DELAY=3

# Retry Settings
MAX_RETRIES=3
RETRY_DELAY=5
