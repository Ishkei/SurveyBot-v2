# API Keys for AI features
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Qmee Login Credentials
QME_EMAIL=your_qmee_email@example.com
QME_PASSWORD=your_qmee_password_here

# LifePoints Login Credentials
LIFEPOINTS_EMAIL=your_lifepoints_email@example.com
LIFEPOINTS_PASSWORD=your_lifepoints_password_here

# Captcha Solving APIs
CAPSOLVER_API_KEY=your_capsolver_api_key_here
ANTI_CAPTCHA_API_KEY=your_anticaptcha_api_key_here

# Enhanced Settings
LOCAL_CAPTCHA_SOLVING=true
HUMANIZE_INTERACTIONS=true
