# AI Settings
AI_MODEL=gemini-1.5-flash-latest
USE_VISION=true
USE_FALLBACK=true

# Timing Settings
PAGE_TIMEOUT=30000
ELEMENT_TIMEOUT=10000
SURVEY_TIMEOUT=300000

# Retry Settings
MAX_RETRIES=3
RETRY_DELAY=5

# Proxy Rotation Settings
ROTATE_PROXY_ON_FAILURE=true
PROXY_ROTATION_INTERVAL=10

# Stealth Settings
RANDOM_DELAYS=true
MIN_DELAY=1.0
MAX_DELAY=3.0
