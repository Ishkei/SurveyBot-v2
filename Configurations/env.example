# Proxy Settings (optional)
PROXY_HOST=
PROXY_PORT=
PROXY_USER=
PROXY_PASS=

# Browser Settings
BROWSER_TYPE=undetected
HEADLESS=false
SLOW_MO=10

# Survey Platform Settings
SURVEY_PLATFORM=qmee
